{
  "version": 2,
  "builds": [
    {
      "src": "./server/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/index.mjs"
    },
    {
      "src": "/api/bookings",
      "dest": "/server/index.mjs"
    },
    {
      "src": "/api/users",
      "dest": "/server/index.mjs"
    }
  ]
}