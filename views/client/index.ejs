                    <div class="dropdown-menu hidden">
                        <a href="/packages/economy">Economy</a>
                        <a href="/packages/business">Business</a>
                        <a href="/packages/first">First</a>
                    </div>
                </div>
                <div class="dropdown py-2 px-4 mr-4">
                    <a class="dropdown" href="/packages">Services</a>
                    <i class="fa-sharp fa-solid fa-caret-down"></i>
                    <div class="dropdown-menu hidden">
                        <a href="/packages/economy">Economy</a>
                        <a href="/packages/business">Business</a>
                        <a href="/packages/first">First</a>
                    </div>
